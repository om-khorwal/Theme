<header>
  <div class="main">
    <h1>{{ shop.name }}</h1>
    <div class="nav">
      {%for link in section.settings.menu.links %}
        <a href="{{ link.url }}">{{ link.title }}</a>
      {% endfor %}
    </div>
  </div>
  {% schema %}
  {
    "name": " Header",
    "settings": [
      {
        "type": "link_list",
        "id": "men",
        "default": "main-menu "
      } 
      
      ]
  }
  {% endschema %}
</header>
